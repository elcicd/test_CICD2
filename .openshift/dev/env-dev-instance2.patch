kind: DeploymentConfig
metadata:
  name: ${APP_NAME}
spec:
  template:
    spec:
      containers:
        - name: ${APP_NAME}
          env:
          - name: password
            valueFrom:
              secretKeyRef:
                key: password
                name: test-sealed-secret
          - name: topicname
            valueFrom:
              configMapKeyRef:
                key: topicname
                name: ${APP_NAME}-configmap
          - name: cgroup
            value: patch-test-dev-instance2
