- op: add
  path: /objects/0/spec/template/spec/containers/0/env
  value:
    - name: password
      valueFrom:
        secretKeyRef:
          key: password
          name: test-cicd2-sealed-secret
    - name: topicname
      valueFrom:
        configMapKeyRef:
          key: topicname
          name: ${APP_NAME}-configmap
    - name: cgroup
      valueFrom:
        configMapKeyRef:
          key: cgroup
          name: ${APP_NAME}-configmap
